{
    "name": "exp-html-skeleton",
    "description": "HTML Skeleton for Expansive",
    "version": "0.1.0",
    "keywords": [
        "expansive-plugin",
        "html"
    ],
    "dependencies": {
        "exp-js": "~0.2",
        "exp-less": "~0.2",
        "exp-css": "~0.2"
    },
    "licenses": "GPL",
    "export": [
        {
            "from": [
                "partials/**",
                "layouts/**",
                "source/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "blend": [
        "expansive.json"
    ],
    "mode": "debug"
}
