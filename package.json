{
    "name": "exp-html-skeleton",
    "description": "HTML Skeleton for Expansive",
    "version": "0.1.0",
    "keywords": [
        "expansive-plugin",
        "html"
    ],
    "dependencies": {
        "exp-less": "~0.1",
        "exp-css": "~0.1"
    },
    "licenses": "GPL",
    "export": [
        {
            "from": [
                "documents/**",
                "partials/**",
                "layouts/**"
            ],
            "to": "${TOP}",
            "overwrite": false
        }
    ],
    "skeleton": true
}
